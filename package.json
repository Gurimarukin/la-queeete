{
  "name": "thequest",
  "version": "1.0.0",
  "description": "La quêêête",
  "targets": {
    "server": {
      "source": "src/server/index.ts",
      "engines": {
        "node": ">= 18"
      }
    },
    "client": {
      "source": "src/client/index.html",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "scripts": {
    "_tsNode": "ts-node -P tsconfig.ts-node.json",
    "clean": "rm -fr .parcel-cache dist",
    "prebuild": "yarn clean && yarn checkEnv",
    "build": "tsc -p tsconfig.build.json && parcel build",
    "checkEnv": "yarn _tsNode src/server/checkEnv.ts && yarn _tsNode src/client/checkEnv.ts",
    "createUser": "yarn _tsNode src/server/createUser.ts",
    "server": "yarn _tsNode src/server/index.ts",
    "~server": "nodemon -e ts,tsx,js,json -w . -w .env --ignore .parcel-cache --ignore dist --ignore src/client --ignore uibook -x \"yarn server\"",
    "~client": "concurrently -k -n parcel,tsc -c green.bold,cyan.bold \"parcel src/client/index.html --dist-dir dist/client\" \"tsc -w --preserveWatchOutput -p tsconfig.client.json\"",
    "compile": "tsc -p tsconfig.json",
    "~compile": "yarn compile -w --preserveWatchOutput",
    "~uibook": "parcel uibook/uibook.html --dist-dir dist/uibook -p 5678",
    "_prettier": "prettier src test *.json *.md *.js",
    "formatCheck": "yarn _prettier --check",
    "format": "yarn _prettier --write",
    "lint": "eslint --cache --max-warnings 0 src test",
    "lintFix": "yarn lint --fix",
    "test": "jest",
    "testUnit": "yarn test --testPathPattern=.*\\.spec\\.ts",
    "testCoverage": "yarn test --ci --coverage",
    "dc": "depcruise src test",
    "dcGraph": "depcruise -v -T dot src -x '(node_modules|fs|util|fp\\.ts)' | dot -T svg | depcruise-wrap-stream-in-html > dependencygraph.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gurimarukin/thequest.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gurimarukin/thequest/issues"
  },
  "homepage": "https://github.com/Gurimarukin/thequest#readme",
  "dependencies": {
    "@phc/argon2": "1.0.9",
    "@popperjs/core": "2.11.8",
    "@react-spring/web": "9.7.3",
    "@use-gesture/react": "10.3.1",
    "cookie": "0.6.0",
    "dayjs": "1.11.10",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "fp-ts": "2.16.5",
    "fp-ts-contrib": "0.1.29",
    "fp-ts-routing": "0.6.0",
    "fp-ts-rxjs": "0.6.15",
    "got": "11.8.6",
    "history": "5.3.0",
    "hyper-ts": "0.7.9",
    "io-ts": "2.2.21",
    "jsdom": "24.0.0",
    "jsonwebtoken": "9.0.2",
    "ky": "0.33.3",
    "lodash.debounce": "4.0.8",
    "lua-in-js": "2.2.0",
    "mongodb": "6.5.0",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "qs": "6.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-popper": "2.3.0",
    "react-select": "5.8.0",
    "rxjs": "6.6.7",
    "rxjs-stream": "3.3.0",
    "swr": "2.2.5",
    "type-fest": "4.14.0",
    "upash": "1.0.2",
    "uuid": "9.0.1",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@type-config/strict": "1.2.1",
    "@types/cookie": "0.6.0",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsdom": "21.1.6",
    "@types/jsonwebtoken": "9.0.6",
    "@types/lodash.debounce": "4.0.9",
